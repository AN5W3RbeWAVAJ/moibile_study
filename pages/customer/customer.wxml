
<view class="page">
  <view class="head">
    <image src="/pages/image/head.jpg"></image>
  </view>
  <scroll-view scroll-y="true" class="list">
      <view class="block" wx:for="{{customerList}}">
        <view>
          <image src="/pages/image/vip.png"></image>
        </view>
        <view class="block_text">
          <text>编号：{{item.khno}}</text>
          <text>名称：{{item.name}}</text>
          <text>电话：{{item.phone}}</text>
        </view>
      </view>
  </scroll-view>
</view>
